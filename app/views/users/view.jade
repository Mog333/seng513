extends ../layout

block append content
    each photo in photos
        .col-md-3
            figure
                img(src="/photos/thumbnail/" + photo.id + ".jpg")
                figcaption
                    |Uploaded by&nbsp;
                    a(href="/users/" + photo.user.id) #{photo.user.name}
                    |&nbsp; #{photo.timeAgo()}

    a(href="/feed?page=" + nextPage) more




